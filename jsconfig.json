{
	"compilerOptions": {
		"outDir": "dist",
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "node",
		"lib": [
			"DOM",
			"DOM.Iterable",
			"ESNext"
		],
		"baseUrl": "./",
		"paths": {
			"@Apis/*": [
				"src/Apis/*"
			],
			"@Assets/*": [
				"src/Assets/*"
			],
			"@Components/*": [
				"src/Components/*"
			],
			"@Enums/*": [
				"src/Enums/*"
			],
			"@Hooks/*": [
				"src/Hooks/*"
			],
			"@Pages/*": [
				"src/Pages/*"
			],
			"@Routes/*": [
				"src/Routes/*"
			],
			"@Store/*": [
				"src/Store/*"
			],
			"@Utils/*": [
				"src/Utils/*"
			],
			"@/*": [
				"src/*"
			]
		},
		"jsx": "react-jsx"
	},
	"include": [
		"src"
	]
}